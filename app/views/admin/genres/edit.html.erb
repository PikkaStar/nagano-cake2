<div class="container">
  <div class="row">
    <div class="col-md-6 mx-auto">
      <h2 class="title">ジャンル編集</h2>
      <%= form_with model: @genre,url: admin_genre_path,method: :patch do |f| %>

        <%= f.label :name,"ジャンル名" %>
        <%= f.text_field :name %><br>
        <%= f.submit "登録" %>

      <% end %>
    </div>
  </div>
</div>